{
  "compilerOptions": {
    "baseUrl": ".",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/actions": ["./src/app/(app)/_actions/"],
      "@/api": ["./src/app/(app)/_api/"],
      "@/blocks": ["./src/app/(app)/_blocks/"],
      "@/components": ["./src/app/(app)/_components/"],
      "@/graphql": ["./src/app/(app)/_graphql/"],
      "@/pages/*": ["./src/app/(app)/(pages)/*"],
      "@/providers": ["./src/app/(app)/_providers/"],
      "@/public/*": ["./public/*"],
      "@/translations": ["./src/app/(app)/_translations/"],
      "@/types": ["./src/app/(app)/_types/"],
      "@/utilities": ["./src/app/(app)/_utilities/"],
      "@/utilities/*": ["./src/app/(app)/_utilities/*"],
      "@/payload/actions": ["./src/payload/actions/"],
      "@/payload/access": ["./src/payload/access/"],
      "@/payload/cells": ["./src/payload/cells/"],
      "@/payload/collections": ["./src/payload/collections/"],
      "@/payload/collections/*": ["./src/payload/collections/*"],
      "@/payload/components": ["./src/payload/components/"],
      "@/payload/data": ["./src/payload/data/"],
      "@/payload/endpoints": ["./src/payload/endpoints/"],
      "@/payload/fields": ["./src/payload/fields/"],
      "@/payload/globals": ["src/payload/globals"],
      "@/payload/hooks": ["src/payload/hooks"],
      "@/payload/templates": ["src/payload/templates"],
      "@/payload/translations": ["src/payload/translations"],
      "@/payload/utilities": ["src/payload/utilities"],
      "@/payload/views": ["src/payload/views"],
      "@payload-types": ["./payload-types.ts"],
      "@payload-config": ["./payload.config.ts"]
    },
    "target": "ES2017"
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
